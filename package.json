{
  "name": "@rschristian/freetunnel",
  "version": "0.2.1",
  "bin": "dist/freetunnel.esm.js",
  "type": "module",
  "license": "MIT",
  "author": "Hristo Dimitrov",
  "description": "Create secure introspectable tunnels to your local device",
  "repository": "https://github.com/rschristian/freetunnel",
  "keywords": [
    "tunnel",
    "webhook",
    "secure",
    "URL",
    "access"
  ],
  "files": [
    "dist",
    "src/views",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "nodemon --watch src --experimental-json-modules --experimental-specifier-resolution=node src/bin.js",
    "start": "node dist/freetunnel.esm.js",
    "build": "premove dist && microbundle -i src/bin.js --target node -f esm --no-sourcemap --compress",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "kleur": "^4.1.3",
    "polka": "^1.0.0-next.11",
    "polka-ejs": "^1.0.0",
    "sade": "^1.7.4",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-rschristian": "^0.1.2",
    "microbundle": "^0.13.0",
    "nodemon": "^2.0.2",
    "premove": "^3.0.1",
    "prettier": "^2.4.1",
    "prettier-config-rschristian": "^0.1.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 12
    },
    "extends": "rschristian"
  },
  "prettier": "prettier-config-rschristian"
}
