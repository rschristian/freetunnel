<!DOCTYPE html>
<html lang="en">
  <head>
      <meta charset="utf-8" />
      <meta name="viewport" content="width=device-width,initial-scale=1" />
      <title>FreeTunnel</title>
  </head>
  <body>
      <h1>Freetunnel</h1>
      <ul>
          <% history.forEach((request, index) => { %>
              <li>
                  <a href="/view/<%=index%>"><%=`${request.method} - ${request.url} - ${request.time}`%></a>
                  <form method="post" action="/view/<%=index%>">
                      <input type="submit" value="Resend">
                  </form>
              </li>
          <% }); %>
          <li></li>
      </ul>
  </body>
</html>
